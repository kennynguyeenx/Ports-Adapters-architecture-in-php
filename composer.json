{
    "name": "kennynguyeenx/library-hexagonal-in-php",
    "type": "project",
    "version": "1.0",
    "description": "Implementation ports & adapters architecture in PHP.",
    "keywords": ["php", "ports", "adapters", "architecture", "library", "hexagonal"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kenny Nguyen",
            "email": "kennynguyeenx@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "sendgrid/sendgrid": "^7.9",
        "guzzlehttp/guzzle": "^7.3",
        "vlucas/phpdotenv": "^5.3",
        "symfony/console": "^5.3",
        "doctrine/orm": "^2.9",
        "php-di/slim-bridge": "^3.1",
        "php-di/symfony-bridge": "^3.0",
        "monolog/monolog": "^2.3",
        "slim/slim": "^4.8",
        "slim/http": "^1.2",
        "doctrine/cache": "^2.1",
        "slim/psr7": "^1.4",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {"Kennynguyeenx\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
